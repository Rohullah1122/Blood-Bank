<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bload_bank">

    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bload_Bank">
        <activity android:name=".activities.Messaged_users"></activity>
        <activity android:name=".activities.message_activity" />
        <activity android:name=".activities.search_bloadgroup" />
        <activity android:name=".activities.user_profile" />
        <activity android:name=".activities.search_activity" />
        <activity android:name=".activities.cre_new_acc" />
        <activity android:name=".activities.activity_moreinfo" />
        <activity android:name=".activities.Main_layout" />
        <activity android:name=".activities.create_acc" />
        <activity android:name=".activities.Login" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>